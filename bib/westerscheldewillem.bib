journal = {Journal of Sea Research},
keywords = {Algal Blooms,Eastern Scheldt,GEM,Grevelingen,North Sea,Oosterschelde,Phaeocystis,Satellite Observations,Westerschelde,primary production,zeespiegel},
mendeley-tags = {Eastern Scheldt,Grevelingen,Oosterschelde,Westerschelde,primary production,zeespiegel},
