@Article{Los2008,
  author = {F J Los and M T Villars and M W M {Van der Tol}},
  title = {{A 3-dimensional primary production model (BLOOM/GEM) and its applications to the (southern) North Sea (coupled physical-chemical-ecological model)}},
  journal = {Journal of Marine Systems},
  year = {2008},
  month = {nov},
  publisher = {Elsevier B.V.},
  volume = {74},
  number = {1-2},
  pages = {259--294},
  keywords = {Eastern Scheldt,Ecological modeling,Model validation,North Sea,Oosterschelde,Primary production,Waddenzee,Westerschelde,primary production,waddenzee,zeespiegel},
  abstract = {This paper presents the ecological modelling instrument BLOOM/GEM and several applications to the southern North Sea, including a 3-dimensional model validation. The current instrument and its predecessors have been used since the 1980s for evaluating the ecological status of the North Sea and potential effect of management strategies. The main modelled processes of nutrient cycling, oxygen dynamics and primary production are described, as well as the external forcings required for the ecological model (hydrodynamics, suspended sediments and river loads). In the development of the BLOOM/GEM modelling instrument, the explicit choice of processes to include (the 'ecological' resolution) has been balanced with the need for high spatial resolution in the model applications for the Dutch coastal zone. The calibration and validation of the BLOOM/GEM modelling instrument as well as the model applications have mainly drawn on the extensive dataset available for the Dutch coastal waters (http://www.waterbase.nl). A specific 3-dimensional model application to the North Sea is described including the model validation results based on the use of an objective cost function for a number of different substances. Plotted model results showing seasonal as well as regional variations and spatial gradients for many substances at several stations give additional support to the validation. As such, the model is well suited to support many management decisions, related to e.g. the OSPAR convention and the European Water Framework Directive and the construction of infrastructural works. {\textcopyright} 2008 Elsevier B.V. All rights reserved.},
  issn = {09247963},
  doi = {10.1016/j.jmarsys.2008.01.002},
  url = {http://linkinghub.elsevier.com/retrieve/pii/S0924796308000274},
  url = {http://linkinghub.elsevier.com/retrieve/pii/S0924796308000274},
  file = {:C\:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Los, Villars, Van der Tol - 2008 - A 3-dimensional primary production model (BLOOMGEM) and its applications to the (southern) North Sea.pdf:PDF},
}

@Article{Provoost2010,
  author = {P Provoost and S {Van Heuven} and K Soetaert and R W P M Laane and J J Middelburg},
  title = {{Seasonal and long-term changes in pH in the Dutch coastal zone}},
  journal = {Biogeosciences},
  year = {2010},
  month = {nov},
  volume = {7},
  number = {11},
  pages = {3869--3878},
  keywords = {Eastern Scheldt,Oosterschelde,Waddenzee,Westerschelde,primary production,waddenzee,zeespiegel},
  abstract = {Recent observations and modelling studies suggest that biogeochemical changes can mask atmospheric CO2-induced pH decreases. Data collected by the Dutch monitoring authorities in different coastal systems (North Sea, Wadden Sea, Ems-Dollard, Eastern Scheldt and Scheldt estuary) since 1975 provide an excellent opportunity to test whether this is the case in the Dutch coastal zone. The time-series were analysed using Multi-Resolution Analysis (MRA) which resulted in the identification of system-dependent patterns on both seasonal and intra-annual time scales. The observed rates of pH change greatly exceed those expected from enhanced CO2 uptake, thus suggesting that other biogeochemical processes, possibly related to changes in nutrient loading, can play a dominant role in ocean acidification. {\textcopyright} Author(s) 2010.},
  issn = {17264170},
  doi = {10.5194/bg-7-3869-2010},
  url = {http://www.biogeosciences.net/7/3869/2010/},
  url = {http://www.biogeosciences.net/7/3869/2010/},
