@Article{Vries1998,
  author = {I {De Vries} and R N M Duin and J C H Peeters and F J Los and M Bokhorst and R W P M Laane},
  title = {{Patterns and trends in nutrients and phytoplankton in Dutch coastal waters: Comparison of time-series analysis, ecological model simulation, and mesocosm experiments}},
  journal = {ICES Journal of Marine Science},
  year = {1998},
  volume = {55},
  number = {4},
  pages = {620--634},
  keywords = {Coastal eutrophication,Eastern Scheldt,Ecological efficiency,Mesocosm experiments,Model simulation,Nutrients (phosphorus, nitrogen, N:P ratio),Oosterschelde,Productivity,Riverine inputs,Trend analysis,Waddenzee,Westerschelde,primary production,waddenzee,zeespiegel},
  abstract = {In the Dutch coastal zone, nutrient and chlorophyll concentrations show gradients of up to one order of magnitude perpendicular to the coast within the first 30-50 km offshore. Time-series analysis reveals significant decreasing trends for dissolved inorganic phosphorus (40%) and total phosphorus (35%) and an increase in the dissolved inorganic N:P ratio from 25-30 to 40-55 over the period 1988-1995. Trends in nitrogen (- 15%), silicate (stable), and chlorophyll are smaller and generally not statistically significant. The trends in phosphorus reflect a proportional and immediate response to decreasing riverine inputs. The observed trends, spatial gradients, and long-term seasonal patterns are simulated quite well with a coupled physical-ecological model with high spatial resolution for the coastal zone. The model results indicate no effect of decreasing phosphorus, but an important role for both nitrogen and light climate in primary production and algal biomass. These results have been reproduced in mesocosm experiments. Moreover, these experiments indicate a strong response of primary production and chlorophyll to nitrogen load, whereas secondary production (macrobenthos) remains relatively stable. Ecological efficiency of secondary production increases from 7% to > 10%, with a decrease in nitrogen loading by 50% from the present level. In the absence of a significant nitrogen reduction in coastal waters, the mesocosm results cannot be related to field data as yet. However, the expectation is that reducing nitrogen inputs will not affect productivity at higher trophic levels to any great extent.},
  issn = {10543139},
  doi = {10.1006/jmsc.1998.0399},
  url = {https://doi.org/10.1006/jmsc.1998.0399},
  file = {:C\:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/De Vries et al. - 1998 - Patterns and trends in nutrients and phytoplankton in Dutch coastal waters Comparison of time-series analysis,.pdf:PDF},
}

@TechReport{Langenberg2008,
  author = {V T Langenberg and T A Troost},
  title = {{Overview of indicators for Good Environmental Status Overview of indicators for Good Environmental Status}},
  year = {2008},
  institution = {Deltares},
  address = {Delft},
  keywords = {Eastern Scheldt,Grevelingen Oosterschelde,Oosterschelde,Waddenzee,Westerschelde,primary production,veersemeer,waddenzee,zeespiegel},
  abstract = {This study was carried out to obtain an overview of the main indicator frameworks with their respective types of indicators used or developed for the monitoring of Marine ecosystems in the Netherlands. We identified a total of 199 indicators belonging to 8 frame works and ranked (scored) them using a qualitative analyses based on the ICES criteria for a good indicators used for technical evaluation. All identified indicators were related (linked) to all GES qualitative descriptors for determining Good Environmental Status (Annex 1 of the MSFDDirective 2008/56/EC). Those GES descriptors with a limited number of relevant indicators were identified for further attention (GES descriptors with information gaps'). A maximum of two high ranked indicators of the least covered GES descriptors were identified. All combined this led to a shortlist of 11 indicators that were analysed for their applicability, scientific soundness, practicality and the probable costs involved with future implementation in GES monitoring. All 11 indicators showed to be of high ecological relevance. The establishment of consistent indicator reference target or limit levels seemed intricate mostly due to lack of or insufficient quality of the quantitative basis for setting such levels. Another underlying difficulty is the insufficient understanding of the complex correlations between pressure and impact in time and space. This hampers especially the full implementation and further development of those indicators meant for the monitoring of impacted biota (e.g., benthos, plankton and seals) and may easily lead to a descriptive and costly monitoring that only gradually in time may allow one to correlate system responses to other external events or vice versa and derive cause-effect relationships. The pressure indicators that monitor physical and chemical phenomena scored higher on the technical evaluation, where unambiguous, and reference as well as limit levels may be constructed and set relatively easily. The time frames for implementation and outcome assessment may furthermore be short and costs are relatively low. It is recommended that a balanced set of indicators may be needed for monitoring the GES of the North Sea ecosystem in the context of conservation, protection, and sustainable use. For successful implementation, some indicators of the set should be scientifically specific and designed to further unravel the ecosystem responses to variable human induced impacts, while other indicators, with less explanatory power, will have to monitor anthropogenic drivers and pressures. The set of indicators must be able to address the different objectives set by ecological, social and economic structures.},
  file = {:C\:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Langenberg, Troost - 2008 - Overview of indicators for Good Environmental Status Overview of indicators for Good Environmental Status.pdf:PDF},
}

